
var idade = prompt("Qual a sua idade? ");

if (idade >= 18){
    alert("maior de idade");
}else{
    alert("Menor de idade");
};


** estrutura de repetição while

var count = 0;
while (count <= 10){
    console.log(count);
    count = count + 1;
    // count ++
};

** Estrutura de repetição for

var count;
for(count=0; count <= 5; count ++){
    alert(count);
};
